<div class="content">
  <h1>
    Office Hours/Lab Queue Statistics
  </h1>
  <!-- <select id="queue_code" class="form-control" name="queue_code" aria-label="Queue Code" required="required">
    <option value="all">All Queues</option>
    {% for queue in viewer.getAllQueuesEver() %}
      <option value="queue {{queue['queue_code']}}">{{queue['queue_code']}}</option>
    {% endfor %}
  </select> -->

  <table class="table">
  <thead>
    <tr>
      {% for key, item in viewer.getQueueStudentData()[0] %}
        {% if key != 'user_id' %}
            <th style="padding:.4rem">{{key}}</th>
        {% endif %}
      {% endfor %}
    </tr>
  </thead>
  <tbody>
    {% for student in viewer.getQueueStudentData() %}
      <tr>
        {% for key, item in student %}
          {% if key != 'user_id' %}
            <td>{{item}}</td>
          {% endif %}
        {% endfor %}
      </tr>
    {% endfor %}
  </tbody>
</table>
</div>
