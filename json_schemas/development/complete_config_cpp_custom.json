            "test*/*.txt",
            "test*/*_diff.json",
            "**/README.txt",
            "input_*.txt",
            "test*/input_*.txt",
            "test01/STDERR.txt",
            "test01/STDOUT.txt",
            "test02/STDOUT.txt",
            "test02/STDERR.txt",
            "test03/STDOUT.txt",
            "test03/STDERR.txt",
            "test04/STDOUT_0.txt",
            "test04/STDOUT_1.txt",
            "test04/STDERR_0.txt",
            "test04/STDERR_1.txt"
        ]
    },
    "autograding_method": "jailed_sandbox",
    "container_options": {
        "container_image": "ubuntu:custom",
        "single_port_per_container": false,
        "use_router": false
    },
    "grading_parameters": {
        "AUTO_POINTS": 20
