{
    "allow_system_calls": [
        "ALLOW_SYSTEM_CALL_CATEGORY_COMMUNICATIONS_AND_NETWORKING_INTERPROCESS_COMMUNICATION",
        "ALLOW_SYSTEM_CALL_CATEGORY_COMMUNICATIONS_AND_NETWORKING_SIGNALS",
        "ALLOW_SYSTEM_CALL_CATEGORY_FILE_MANAGEMENT_MOVE_DELETE_RENAME_FILE_DIRECTORY",
        "ALLOW_SYSTEM_CALL_CATEGORY_FILE_MANAGEMENT_RARE",
        "ALLOW_SYSTEM_CALL_CATEGORY_PROCESS_CONTROL_NEW_PROCESS_THREAD",
        "ALLOW_SYSTEM_CALL_CATEGORY_PROCESS_CONTROL_SCHEDULING",
        "ALLOW_SYSTEM_CALL_CATEGORY_UNKNOWN"
    ],
    "autograding": {
        "compilation_to_runner": [
            "**/*.out",
            "**/*.class"
        ],
        "compilation_to_validation": [
            "test*/STDOUT*.txt",
            "test*/STDERR*.txt"
        ],
        "submission_to_compilation": [
            "**/*.cpp",
            "**/*.cxx",
            "**/*.c",
            "**/*.h",
            "**/*.hpp",
            "**/*.hxx",
            "**/*.java"
        ],
        "submission_to_runner": [
            "*.m"
        ],
        "submission_to_validation": [
            "**/README.txt",
            "input_*.txt",
            "**/*.pdf"
        ],
        "use_checkout_subdirectory": "",
        "work_to_details": [
            "*.png",
            "test01/student1.png",
            "test01/student1.png",
            "test01/STDOUT.txt",
            "test01/STDERR.txt"
        ]
    },
    "autograding_method": "jailed_sandbox",
    "container_options": {
        "container_image": "ubuntu:custom",
        "single_port_per_container": false,
        "use_router": false
    },
    "resource_limits": {
        "RLIMIT_AS": "RLIM_INFINITY",
        "RLIMIT_CPU": 60,
        "RLIMIT_LOCKS": 1000000,
        "RLIMIT_NOFILE": 1000000,
        "RLIMIT_NPROC": 100
    },
    "testcases": [
        {
            "containers": [
                {
                    "commands": [
                        "matlab -nodisplay -automation -r 'screenshot_plot'"
                    ],
                    "container_image": "ubuntu:custom",
                    "container_name": "container0",
                    "outgoing_connections": [],
                    "server": false
                }
            ],
            "dispatcher_actions": [],
            "points": 1,
            "pre_commands": [],
            "single_port_per_container": false,
            "title": "matlab_test",
            "use_router": false,
            "validation": [
                {
                    "actual_file": "student1.png",
                    "deduction": 0.5,
                    "description": "student_image_one",
                    "method": "fileExists",
                    "show_actual": "always",
                    "show_message": "always"
                },
                {
                    "acceptable_threshold": ".01",
                    "actual_file": "student1.png",
                    "deduction": 0.5,
                    "description": "Diff Check",
                    "expected_file": "professor1.png",
                    "method": "ImageDiff",
                    "show_actual": "always",
                    "show_difference_image": "always",
                    "show_expected": "always",
                    "show_message": "always"
                },
                {
                    "actual_file": "STDOUT.txt",
                    "deduction": 0.0,
                    "description": "Standard Output (STDOUT.txt)",
                    "method": "warnIfNotEmpty",
                    "show_actual": "on_failure",
                    "show_message": "on_failure"
                },
                {
                    "actual_file": "STDERR.txt",
                    "deduction": 0.0,
                    "description": "Standard Error (STDERR.txt)",
                    "method": "warnIfNotEmpty",
                    "show_actual": "on_failure",
                    "show_message": "on_failure"
                }
            ]
        },
        {
            "max_submissions": 20,
            "penalty": -0.1,
            "points": -5,
            "title": "Submission Limit",
            "type": "FileCheck"
        }
    ]
}
