{
    "assignment_message": "Please upload a single .pdf file containing 500-1000 words.",
    "autograding": {
        "compilation_to_runner": [
            "**/*.out",
            "**/*.class"
        ],
        "compilation_to_validation": [
            "test*/STDOUT*.txt",
            "test*/STDERR*.txt"
        ],
        "submission_to_compilation": [
            "*.pdf"
        ],
        "submission_to_runner": [
            "*.pdf"
        ],
        "submission_to_validation": [
            "*.pdf"
        ],
        "use_checkout_subdirectory": "",
        "work_to_details": [
            "test*/*.txt",
            "test*/*_diff.json",
            "**/README.txt",
            "input_*.txt",
            "test*/input_*.txt",
            "test01/*.pdf",
            "test02/STDOUT_1.txt",
            "test02/STDOUT_1.txt",
            "test02/plaintext.txt",
            "test02/STDOUT_0.txt",
            "test02/STDERR_0.txt",
            "test02/STDERR_1.txt"
        ]
    },
    "autograding_method": "jailed_sandbox",
    "container_options": {
        "container_image": "ubuntu:custom",
        "single_port_per_container": false,
        "use_router": false
    },
    "max_submission_size": 1000000,
    "testcases": [
        {
            "containers": [
                {
                    "commands": [],
                    "container_image": "ubuntu:custom",
                    "container_name": "container0",
                    "outgoing_connections": [],
                    "server": false
                }
            ],
            "dispatcher_actions": [],
            "points": 1,
            "pre_commands": [],
            "single_port_per_container": false,
            "title": "Submitted a .pdf file",
            "type": "FileCheck",
            "use_router": false,
            "validation": [
                {
                    "actual_file": "*.pdf",
                    "deduction": 1.0,
                    "description": "*.pdf",
                    "method": "fileExists",
                    "show_actual": "always",
                    "show_message": "always"
                }
            ]
        },
        {
            "containers": [
                {
                    "commands": [
                        "pdftotext *.pdf plaintext.txt",
                        "wc -w < plaintext.txt"
                    ],
                    "container_image": "ubuntu:custom",
                    "container_name": "container0",
                    "outgoing_connections": [],
                    "server": false
                }
            ],
            "dispatcher_actions": [],
            "points": 1,
            "pre_commands": [],
            "single_port_per_container": false,
            "title": "Required 500-1000 Words",
            "use_router": false,
            "validation": [
                {
                    "actual_file": "STDOUT_1.txt",
                    "comparison": "ge",
                    "deduction": 1,
                    "description": "Word Count",
                    "failure_message": "Too few words",
                    "method": "intComparison",
                    "show_actual": "on_failure",
                    "show_message": "on_failure",
                    "term": 500
                },
                {
                    "actual_file": "STDOUT_1.txt",
                    "comparison": "le",
                    "deduction": 1,
                    "description": "Word Count",
                    "failure_message": "Too many words",
                    "method": "intComparison",
                    "show_actual": "on_failure",
                    "show_message": "on_failure",
                    "term": 1000
                },
                {
                    "actual_file": "plaintext.txt",
                    "deduction": 1,
                    "description": "plaintext.txt",
                    "method": "fileExists",
                    "show_actual": "always",
                    "show_message": "always"
                },
                {
                    "actual_file": "STDOUT_0.txt",
                    "deduction": 0.0,
                    "description": "Standard Output (STDOUT_0.txt)",
                    "method": "warnIfNotEmpty",
                    "show_actual": "on_failure",
                    "show_message": "on_failure"
                },
                {
                    "actual_file": "STDERR_0.txt",
                    "deduction": 0.0,
                    "description": "Standard Error (STDERR_0.txt)",
                    "method": "warnIfNotEmpty",
                    "show_actual": "on_failure",
                    "show_message": "on_failure"
                },
                {
                    "actual_file": "STDERR_1.txt",
                    "deduction": 0.0,
                    "description": "Standard Error (STDERR_1.txt)",
                    "method": "warnIfNotEmpty",
                    "show_actual": "on_failure",
                    "show_message": "on_failure"
                }
            ]
        },
        {
            "max_submissions": 20,
            "penalty": -0.1,
            "points": -5,
            "title": "Submission Limit",
            "type": "FileCheck"
        }
    ]
}
